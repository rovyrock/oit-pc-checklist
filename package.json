{
  "name": "oit-pc-checklist",
  "private": true,
  "homepage": ".",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "theme": "ppd-theme -w"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@types/react-router-dom": "^5.1.7",
    "@umijs/preset-react": "1.x",
    "axios": "^0.21.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "oit-permission": "^0.1.6",
    "oit-style": "^1.0.26",
    "react-hook-form": "^7.17.4",
    "react-router-dom": "^5.2.0",
    "tea-chart": "^2.4.2",
    "tea-component": "^2.6.9",
    "umi": "^3.3.5"
  },
  "devDependencies": {
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@umijs/plugin-qiankun": "^2.20.0",
    "@umijs/test": "^3.3.5",
    "lint-staged": "^10.0.7",
    "ppd-theme": "^1.0.2",
    "prettier": "^2.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
